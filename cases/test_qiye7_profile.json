        "params": "page_size=999&search=",
        "method": "get",
        "assert": {
            "code":200,
            "count": 3
        },
        "desc": "测试企业6权限查询"
    },

    "test_2post_profile": {
        "url": "http://192.168.3.18:30236/account/profile/",
        "json": {"role": "admin", "phone": "13200000002", "name": "1"},
        "method": "post",
        "assert": {
            "code":201,
            "phone": "13200000002"
        },
        "desc": "测试企业6权限增加"
    },
   

    

    "test_3patch_profile": {
        "url": "http://192.168.3.18:30236/account/profile/120/",
